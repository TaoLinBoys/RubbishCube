void fillIn(int i){
  if(i == 0){
    fill(255,255,255);//white front
    
  }else if(i == 1){
    fill(255,255,0);  //yellow back
    
  }else if(i == 2){
    fill(0,0,255);    //blue right
    
  }else if(i == 3){
    fill(0,255,0);    //green left
    
  }else if(i == 4){
    fill(255,0,0);    //red down
    
  }else if(i == 5){
    fill(255,69,0);   //orange top
  }
}