void fillIn(int i){
  if(i == 0){
    fill(255,255,255);//white front
    
  }else if(i == 1){
    fill(0,81,186);  //blue right
    
  }else if(i == 2){
    fill(0,158,96);    //green left
    
  }else if(i == 3){
    fill(196,30,58);    //red down
    
  }else if(i == 4){
    fill(255,88,0);    //orange top
    
  }else if(i == 5){
    fill(255,213,0);   //yellow back
  }
}